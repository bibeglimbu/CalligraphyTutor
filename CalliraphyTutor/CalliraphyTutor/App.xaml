<Application x:Class="CalligraphyTutor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CalligraphyTutor"
             xmlns:localViewModel="clr-namespace:CalligraphyTutor.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <localViewModel:MainWindowViewModel x:Key="MainWindowViewModel" />
        <localViewModel:ExpertViewModel x:Key="ExpertViewModel" />
        <localViewModel:StudentViewModel x:Key="StudentViewModel" />
        <localViewModel:ResultsViewModel x:Key="ResultsViewModel"/>

    </Application.Resources>
</Application>
